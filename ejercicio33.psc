	Definir i, vector Como Entero;
	Dimension vector[30];
	
	Para i=0 Hasta 30-1 Con Paso 1 Hacer
		vector[i] = 3*i+1;
		Mostrar "[",vector[i],"] -" Sin Saltar;
	Fin Para
	
